            <geom density="800"/>

            <default class="visual">
                <geom type="mesh" group="1" contype="0" conaffinity="0"/>

                <!--Robot Gripper-->
                <default class="visual_mount">
                    <geom mesh="mount" />
                </default>

                <default class="visual_base_dg">
                    <geom mesh="base_link" />
                </default>
                <default class="visual_l_1_1">
                    <geom mesh="l_1_1" />
                </default>
                <default class="visual_l_1_2">
